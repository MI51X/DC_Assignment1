<Window x:Class="ClientGUI.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientGUI"
        mc:Ignorable="d"
        Title="Dashboard" Height="400" Width="300">

    <Grid Margin="12,108,12,99"  RenderTransformOrigin="0.405,0.067" HorizontalAlignment="Center" VerticalAlignment="Center">
        <TextBlock x:Name="Namet" Margin="0,0,213,119" Text="Name" />

        <Button Content="All Services" Margin="34,113,158,0" Click="AllServices_Click" Height="36" VerticalAlignment="Top"/>

        <Button Content="Log Out" Margin="158,113,34,0" Click="Logout_Click" Height="36" VerticalAlignment="Top"/>

        <ProgressBar x:Name="progress"  Minimum="0" Maximum="100"  Margin="37,69,37,0" RenderTransformOrigin="0.5,0.5" Height="20" VerticalAlignment="Top"  />
    </Grid>

</Window>
