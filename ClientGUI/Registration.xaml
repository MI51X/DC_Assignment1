<Window x:Class="ClientGUI.Registration"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientGUI"
        mc:Ignorable="d"
        Title="Registration" Height="400" Width="300">

    <Grid Margin="12,108,12,99">
        <TextBlock Margin="0,0,213,119" Text="Name" />
        <TextBox x:Name="NameBox" Margin="67,0,6,0" Height="30" VerticalAlignment="Top" />
        <TextBox x:Name="PasswordBox" Margin="68,53,6,0" RenderTransformOrigin="0.5,0.5" Height="30" VerticalAlignment="Top" />
        <Button Content="Register" Margin="34,111,158,0" Click="Register_Click" Height="36" VerticalAlignment="Top"/>
        <TextBlock Margin="0,55,208,66" Text="Password" />
        <Button Content="Back" Margin="158,111,34,0" Click="Back_Click" Height="36" VerticalAlignment="Top"/>
    </Grid>

</Window>
